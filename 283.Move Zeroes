#include <iostream>
#include<vector>

using namespace std;






int main()
{
	vector<int> num = {0,1,0,3,12,0,1,0,3,12};
	int count = 0;


	
	for (auto i=0; i<num.size();i++) {
		if (num[i]==0){
			//bc the parameter passed to the erase function must be an iterator
			//num.begin+i position is the iterator at index position i
			num.erase(num.begin()+i);
			count++;	


			
		
		}
	}

	if (count != 0) {
		for (int a = 0; a < count; a++) {
			num.push_back(0);
		
		
		}
	
	
	
	}

	for (int u = 0; u < num.size(); u++) {
	
		cout << num[u] << endl;

	}




	
	

}





